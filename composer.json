{
    "name": "sebastianl/warren",
    "description": "A simple, object-oriented library for creating RabbitMQ service workers.",
    "authors": [
        {
            "name": "Sebastian Lenartowicz",
            "email": "sebastian.lenartowicz.one@gmail.com"
        }
    ],
    "version": "0.1.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Warren\\": "src",
            "Warren\\Test\\": "test"
        }
    },
    "require": {
        "psr/http-message": "*",
        "php": ">=7.1.0",
        "guzzlehttp/psr7": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "squizlabs/php_codesniffer": "*",
        "overtrue/phplint": "^1.0",
        "sensiolabs/security-checker": "*"
    },
    "suggest": {
        "phpamqplib/phpamqplib": "To use the PhpAmqpLib connection adapter"
    }
}
