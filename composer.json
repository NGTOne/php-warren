{
    "name": "warren/warren",
    "description": "A simple, object-oriented library for creating RabbitMQ service workers.",
    "keywords": ["rabbitmq", "message", "queue"],
    "homepage": "https://github.com/NGTOne/warren",
    "authors": [
        {
            "name": "Sebastian Lenartowicz",
            "email": "sebastian.lenartowicz.one@gmail.com"
        }
    ],
    "version": "1.0.0",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Warren\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Warren\\Test\\": "test"
        }
    },
    "require": {
        "psr/http-message": "1.0.*",
        "php": ">=7.1.0",
        "guzzlehttp/psr7": "1.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "squizlabs/php_codesniffer": "*",
        "overtrue/phplint": "^1.0",
        "sensiolabs/security-checker": "*",
        "php-amqplib/php-amqplib": "2.*"
    },
    "suggest": {
        "php-amqplib/php-amqplib": "To use the PhpAmqpLib connection adapter"
    }
}
